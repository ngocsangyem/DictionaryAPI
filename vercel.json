{
	"version": 2,
	"builds": [
		{
			"src": "./src/app.js",
			"use": "@vercel/node"
		},
		{
			"src": "./src/public/",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "/api/(.*)",
			"dest": "src/app.js",
			"headers": {
				"Access-Control-Allow-Origin": "*",
				"Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
				"Access-Control-Allow-Credentials": "true",
				"Cache-Control": "public, max-age=31556952, immutable"
			},
			"continue": true
		},
		{
			"src": "/api/(.*)",
			"methods": [
				"OPTIONS"
			],
			"dest": "src/cors.js"
		},
		{
			"src": "/",
			"dest": "src/app.js"
		},
		{
			"src": "/(.+)",
			"dest": "src/public/$1",
			"headers": {
				"Cache-Control": "public, max-age=31556952, immutable"
			}
		}
	]
}
